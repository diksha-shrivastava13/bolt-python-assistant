{
  "_metadata": {
        "major_version": 1,
        "minor_version": 1
    },
  "display_information": {
    "name": "Bolt AI Assistant"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": true,
      "messages_tab_enabled": true,
      "messages_tab_read_only_enabled": false
    },
    "bot_user": {
      "display_name": "Bolt AI Assistant",
            "always_online": true
    },
    "slash_commands": [
      {
        "command": "/ask-bolty",
        "description": "Interact with Bolty.",
        "should_escape": false
      }
    ],
    "assistant_view": {
      "assistant_description": "Hi, I'm an assistant built by AI Makerspace. Feel free to shoot me any questions.",
      "suggested_prompts": []
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "app_mentions:read",
        "channels:history",
        "channels:read",
        "chat:write",
        "chat:write.public",
        "commands",
        "groups:history",
        "groups:read",
        "im:history",
        "im:read",
        "im:write",
        "mpim:history",
        "mpim:read",
        "mpim:write",
        "users:read",
        "assistant:write",
        "channels:join",
        "chat:write.customize"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "bot_events": [
        "app_home_opened",
        "app_mention",
        "function_executed",
        "message.channels",
        "message.groups",
        "message.mpim",
        "message.im",
        "assistant_thread_context_changed",
        "assistant_thread_started"
      ]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}